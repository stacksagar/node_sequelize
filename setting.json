{
  // VS Code automatically highlights selected words.
  // "vim.hlsearch": false,
  "vim.timeout": 200,
  "vim.useSystemClipboard": true,
  "vim.insertModeKeyBindings": [
    {
      "before": ["k", "j"],
      "after": ["<Esc>", "l"]
    }
  ],
  "vim.useCtrlKeys": true,
  "vim.handleKeys": {
    "<C-f>": true,
    "<C-a>": false
  },
  "vim.normalModeKeyBindings": [
    {
      "before": ["v", "n"],
      "after": ["v", "g", "_", "y"]
    },
    {
      "before": ["d", "n"],
      "after": ["d", "g", "_"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<C-n>"],
      "commands": [":nohl"]
    }
  ],
  // vim mode viwy viwcmd+d p issue hack
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["p"],
      "commands": ["editor.action.clipboardPasteAction"]
    }
  ],
  "css.lint.unknownProperties": "ignore",
  "css.lint.unknownVendorSpecificProperties": "ignore",
  "css.lint.compatibleVendorPrefixes": "ignore",
  "css.lint.vendorPrefix": "ignore",
  "tailwindCSS.lint.invalidTailwindDirective": "ignore",
  "css.lint.boxModel": "ignore",
  "css.lint.propertyIgnoredDueToDisplay": "ignore",
  "scss.lint.compatibleVendorPrefixes": "ignore",
  "css.lint.argumentsInColorFunction": "ignore",
  "css.lint.float": "ignore",
  "css.lint.duplicateProperties": "ignore",
  "css.lint.fontFaceProperties": "ignore",
  "css.lint.hexColorLength": "ignore",
  "css.lint.idSelector": "ignore",
  "css.lint.ieHack": "ignore",
  "css.lint.important": "ignore",
  "css.lint.importStatement": "ignore",
  "css.lint.universalSelector": "ignore",

  "editor.fontSize": 22,
  "editor.wordWrap": "on",
  "editor.lineNumbers": "on",
  "editor.codeLens": false,
  "editor.folding": true,
  "editor.glyphMargin": false,
  "editor.formatOnSave": true,
  "editor.hover.enabled": true,
  "editor.fontLigatures": true,
  "editor.fontFamily": "monolisa, dank mono, operator mono",
  "editor.minimap.enabled": false,
  "editor.cursorBlinking": "expand",
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorSurroundingLinesStyle": "all",
  "editor.find.cursorMoveOnType": true,
  "editor.multiCursorMergeOverlapping": true,
  "editor.multiCursorPaste": "full",
  "editor.renderWhitespace": "none",
  "editor.suggestSelection": "first",
  "editor.renderControlCharacters": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
  "editor.renderLineHighlight": "all",

  "explorer.compactFolders": false,

  "files.trimTrailingWhitespace": true,
  "files.exclude": {
    "**/node_modules": true,
    "**/package-lock.json": true
    // Hide js files after running tsc.
    // "**/*.js": { "when": "$(basename).ts" }
  },

  "search.searchOnType": false,

  "telemetry.telemetryLevel": "off",

  "terminal.integrated.fontSize": 13,
  "terminal.integrated.defaultProfile.osx": "zsh",
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.fontWeightBold": "normal",
  "terminal.integrated.drawBoldTextInBrightColors": false,
  "terminal.integrated.gpuAcceleration": "on",

  "typescript.updateImportsOnFileMove.enabled": "always",

  "vsintellicode.modify.editor.suggestSelection": "choseToUpdateConfiguration",
  "workbench.editor.enablePreviewFromQuickOpen": false,

  // Use vim hjkl to navigate folders.
  "workbench.list.automaticKeyboardNavigation": false,

  "zenMode.hideTabs": true,
  "zenMode.hideLineNumbers": true,
  "editor.guides.indentation": false,
  "workbench.colorCustomizations": {
    "terminal.foreground": "#cccccc",
    "terminal.background": "#070808",

    "quickInput.background": "#1d2021",
    "quickInputList.focusBackground": "#070808",

    "editor.background": "#070808",
    "sideBar.background": "#070808",
    "tab.activeBackground": "#070808",
    "statusBar.background": "#070808",
    "activityBar.background": "#070808",

    "editorLineNumber.activeForeground": "#cccccc"
  },
  "workbench.iconTheme": "material-icon-theme",
  "workbench.tree.renderIndentGuides": "none",
  "screencastMode.fontSize": 20,
  "editor.cursorWidth": 2,
  "editor.cursorSurroundingLines": 2,
  "editor.hideCursorInOverviewRuler": true,
  "editor.codeLensFontFamily": "dank mono",
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "explorer.confirmDragAndDrop": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "explorer.confirmDelete": false,
  "security.workspace.trust.untrustedFiles": "open",
  "liveServer.settings.donotShowInfoMsg": true,
  "workbench.editorAssociations": {
    "*.json": "default",
    "*.md": "vscode.markdown.preview.editor"
  },
  "css.lint.emptyRules": "ignore",
  "liveServer.settings.donotVerifyTags": true,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "editor.tabSize": 2,
  "debug.terminal.clearBeforeReusing": true,
  "terminal.integrated.allowMnemonics": true,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "files.autoSaveDelay": 5000,
  "css.lint.unknownAtRules": "ignore",
  "files.autoSave": "afterDelay",
  "cSpell.enabled": false,
  "workbench.colorTheme": "Winter is Coming (Dark Black)",
  "easycode.userEmail": "stacksagar@gmail.com",
  "thunder-client.httpLibrary": "axios",
  "[php]": {
    "editor.defaultFormatter": "kokororin.vscode-phpfmt"
  },
  "CodeGPT.model": "gpt-4",
  "CodeGPT.temperature": 1,
  "git.openRepositoryInParentFolders": "never",
  "php.files.exclude": [],
  "vscode-edge-devtools.webhint": false,
  "window.zoomLevel": -1
}
